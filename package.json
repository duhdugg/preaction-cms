{
  "name": "@preaction/cms",
  "description": "Preaction CMS is a barebones, customizeable CMS built on top of simple libraries.",
  "version": "0.8.1",
  "proxy": "http://localhost:8999",
  "dependencies": {
    "@preaction/validation": "^1.3.4",
    "animate.css": "^3.7.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "connect-session-sequelize": "^6.1.1",
    "cookie-parser": "^1.4.4",
    "es5-shim": "^4.5.13",
    "es6-shim": "^0.35.5",
    "excerpt-html": "^1.2.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "hasha": "^5.2.0",
    "jpeg-autorotate": "^5.0.3",
    "mobile-detect": "^1.4.4",
    "multer": "^1.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-scripts": "^3.4.0",
    "sequelize": "^5.21.5",
    "sitemap": "^5.1.0",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@preaction/bootstrap-clips": "^1.4.10",
    "@preaction/inputs": "^1.3.14",
    "bootstrap": "^4.4.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "hex-rgb": "^4.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "react-router-dom": "^5.1.2"
  },
  "scripts": {
    "backup-db": "PREACTION_DB_BACKUP=1 node -e \"require('./lib/db.js').backup()\"",
    "clear-sessions": "node scripts/clear-sessions.js",
    "dev-client": "react-scripts start",
    "build": "react-scripts build",
    "makeover": "prettier --write ./*.js src/*.j* src/lib/* lib/*.js lib/modules/*.js scripts/*.js && yarn test",
    "test": "prettier -c ./*.js src/*.j* src/lib/* lib/*.js lib/modules/*.js scripts/*.js && eslint ./*.js src/*.j* src/lib/* lib/*.js lib/modules/*.js scripts/*.js && react-scripts test --coverage",
    "eject": "react-scripts eject",
    "dev-server": "nodemon -i src -i scripts server.js",
    "randomize-password": "node scripts/randomize-password.js",
    "set-password": "bash scripts/set-password.sh",
    "start-inspect": "node --inspect server.js",
    "start-profile": "node --prof server.js",
    "start": "node server.js",
    "package": "bash scripts/package.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Doug Elkin <doug@dougelkin.com> (https://dougelkin.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/dougelkin/preaction-cms.git"
  }
}
